<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.notesapplicationlivedata">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="Easy Notes"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.NotesApplicationLiveData">
        <activity android:name=".Activity.UpdateNotesActivity">
        </activity>
        <activity android:name=".Activity.InsertNotesActivity" />
        <activity android:name=".Activity.SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>